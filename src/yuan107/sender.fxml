<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<BorderPane xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="yuan107.Receiver">
    <center>
        <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="TOP_CENTER">
            <BorderPane.margin>
                <Insets />
            </BorderPane.margin>
            <content>
                <ListView id="listViewConnected" fx:id="listviewUsers" />
            </content>
        </ScrollPane>
    </center>
    <bottom>
        <HBox BorderPane.alignment="CENTER">
            <children>
                <VBox HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </HBox.margin>
                    <children>
                        <ComboBox id="comboSource" prefWidth="9999.0" promptText="Select Audio Source..." />
                        <ComboBox id="comboReceiver" prefWidth="9999.0" promptText="Select Receiver..." />
                    </children>
                </VBox>
                <Button defaultButton="true" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" text="Start Sending">
                    <HBox.margin>
                        <Insets bottom="10.0" right="10.0" top="10.0" />
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
    </bottom>
</BorderPane>
